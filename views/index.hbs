<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

<script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
<script src="javascripts/jquery.paginate.js"></script>
<script src="javascripts/main.js"></script>

<body>
	{{#if err}}
		<h1>{{err}}</h1>
	{{/if}}
	<ul class="ticket-list">
		{{#each tickets}}		
			<a href="ticket={{@index}}">
				<li>{{subject}}</li>
			</a>
		{{/each}}	
	</ul>
</body>
